<h1>Listing occurences</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Device brand</th>
      <th>Device model</th>
      <th>Operation system version</th>
      <th>App version</th>
      <th>Assertion</th>
      <th>Excp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @occurences.each do |occurence| %>
      <tr>
        <td><%= occurence.date %></td>
        <td><%= occurence.device_brand %></td>
        <td><%= occurence.device_model %></td>
        <td><%= occurence.operation_system_version %></td>
        <td><%= occurence.app_version %></td>
        <td><%= occurence.assertion %></td>
        <td><%= occurence.excp %></td>
        <td><%= link_to 'Show', occurence %></td>
        <td><%= link_to 'Edit', edit_occurence_path(occurence) %></td>
        <td><%= link_to 'Destroy', occurence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Occurence', new_occurence_path %>
