<%= simple_form_for(@occurence) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :device_brand %>
    <%= f.input :device_model %>
    <%= f.input :operation_system_version %>
    <%= f.input :app_version %>
    <%= f.association :assertion %>
    <%= f.association :excp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
